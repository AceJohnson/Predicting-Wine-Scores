    "region = list(train.region_1.value_counts()[:50].index)\n",
    "def region_to_simple(x,region):\n",
    "    if x in region:\n",
    "        return x\n",
    "    else: \n",
    "        return 'Other'\n",
    "train['region_simple'] = train.region_1.apply(lambda x: prov_to_simple(x,region))\n",
